{
  "name": "foto-render-frontend",
  "version": "2.0.0",
  "private": true,
  "scripts": {
    "dev": "next dev --port 3000",
    "dev:network": "next dev -H 0.0.0.0 --port 3000",
    "dev:turbo": "next dev --turbo --port 3000",
    "dev:verbose": "next dev --port 3000 --verbose",
    "build": "next build",
    "build:analyze": "ANALYZE=true next build",
    "start": "next start --port 3000",
    "start:production": "next start --port 3000 --hostname 0.0.0.0",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "clean": "rm -rf .next node_modules/.cache",
    "backend:start": "cd ../backend && npm run start",
    "backend:stop": "cd ../backend && npm run stop",
    "backend:status": "cd ../backend && npm run status",
    "system:health": "curl -s http://localhost:8000/health || echo 'Backend not running'",
    "system:check": "curl -s http://localhost:8000/health > /dev/null && echo '✅ Backend is healthy' || echo '❌ Backend is down - run: npm run backend:start'"
  },
  "dependencies": {
    "@tanstack/react-table": "^8.21.3",
    "clsx": "^2.1.1",
    "next": "^15.3.4",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-dropzone": "^14.3.8",
    "tailwind-merge": "^3.3.1"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3",
    "@tailwindcss/line-clamp": "^0.4.4",
    "@types/node": "^20",
    "@types/react": "^19.1.8",
    "@types/react-dom": "^19.1.6",
    "@typescript-eslint/eslint-plugin": "^8.36.0",
    "@typescript-eslint/parser": "^8.36.0",
    "autoprefixer": "^10.4.21",
    "eslint": "^9",
    "eslint-config-airbnb-base": "^15.0.0",
    "eslint-config-next": "^15.3.4",
    "postcss": "^8.5.4",
    "tailwindcss": "^3.4.17",
    "typescript": "^5"
  }
}
